{
	"name": "md-graph",
	"displayName": "md-graph",
	"description": "Visualize your markdown files as a network graph. Easily navigate files and see relationships.",
	"version": "0.0.1",
	"publisher": "ianjsikes",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"markdown",
		"foam"
	],
	"bugs": {
		"url": "https://github.com/ianjsikes/md-graph/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ianjsikes/md-graph"
	},
	"license": "MIT",
	"activationEvents": [
		"onLanguage:markdown",
		"onCommand:md-graph.showGraph"
	],
	"main": "./extension/out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "md-graph.showGraph",
				"title": "md-graph: Show Graph"
			}
		],
		"configuration": {
			"title": "md-graph",
			"properties": {}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production --config extension/webpack.config.js",
		"start": "webpack --mode development --config extension/webpack.config.js",
		"watch": "webpack --mode development --watch --config extension/webpack.config.js",
		"lint": "eslint extension/src --ext ts"
	},
	"prettier": {
		"semi": false,
		"arrowParens": "always",
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@types/glob": "^7.1.1",
		"@types/md5": "^2.2.0",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"babel-loader": "^8.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.44.0",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"md5": "^2.2.1"
	}
}
