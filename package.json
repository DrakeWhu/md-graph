{
	"name": "md-graph",
	"displayName": "md-graph",
	"description": "Visualize your markdown files as a network graph. Easily navigate files and see relationships.",
	"version": "0.0.1",
	"publisher": "ianjsikes",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"markdown",
		"foam"
	],
	"bugs": {
		"url": "https://github.com/ianjsikes/md-graph/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ianjsikes/md-graph"
	},
	"license": "MIT",
	"activationEvents": [
		"onLanguage:markdown",
		"onCommand:md-graph.helloWorld"
	],
	"main": "./extension/out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "md-graph.showGraph",
				"title": "md-graph: Show Graph"
			}
		],
		"configuration": {
			"title": "md-graph",
			"properties": {}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc --project ./extension",
		"lint": "eslint extension/src --ext ts",
		"watch": "tsc -watch --project ./extension",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./extension/out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
