{
	"name": "md-graph",
	"displayName": "md-graph",
	"description": "Visualize your markdown files as a network graph. Easily navigate files and see relationships.",
	"version": "0.0.1",
	"publisher": "ianjsikes",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"markdown",
		"foam"
	],
	"bugs": {
		"url": "https://github.com/ianjsikes/md-graph/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ianjsikes/md-graph"
	},
	"license": "MIT",
	"activationEvents": [
		"onLanguage:markdown",
		"onCommand:md-graph.showGraph"
	],
	"main": "./extension/out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "md-graph.showGraph",
				"title": "md-graph: Show Graph"
			}
		],
		"configuration": {
			"title": "md-graph",
			"properties": {
				"md-graph.showColumn": {
					"type": "string",
					"default": "beside",
					"description": "- active – in the currently focused column.\n- beside – (default) other than the current.\n- one, two, three, four, five, six, seven, eight, nine – respective editor columns."
				},
				"md-graph.openColumn": {
					"type": "string",
					"default": "one",
					"description": "- active – in the currently focused column.\n- beside – other than the current.\n- one (default), two, three, four, five, six, seven, eight, nine – respective editor columns."
				},
				"md-graph.autoStart": {
					"type": "boolean",
					"default": false,
					"description": "Should Markdown Links automatically start when a markdown file is opened."
				},
				"md-graph.fileTypes": {
					"type": "array",
					"default": [
						"md"
					],
					"description": "File type extensions that should be parsed by md-graph"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production --config extension/webpack.config.js",
		"start": "webpack --mode development --config extension/webpack.config.js",
		"watch": "concurrently -r \"yarn watch:webview\" \"yarn watch:extension\"",
		"lint": "eslint extension/src --ext ts",
		"build:webview": "webpack --mode development --config webview/webpack.config.js",
		"build:extension": "webpack --mode development --config extension/webpack.config.js",
		"watch:webview": "webpack --mode development --config webview/webpack.config.js --watch",
		"watch:extension": "webpack --mode development --config extension/webpack.config.js --watch"
	},
	"prettier": {
		"semi": false,
		"arrowParens": "always",
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/plugin-transform-runtime": "^7.10.5",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@types/d3": "^5.7.2",
		"@types/glob": "^7.1.1",
		"@types/md5": "^2.2.0",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/react": "^16.9.44",
		"@types/react-dom": "^16.9.8",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"babel-loader": "^8.1.0",
		"concurrently": "^5.2.0",
		"css-loader": "^4.2.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"style-loader": "^1.2.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.44.0",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"@react-spring/core": "^9.0.0-rc.3",
		"@react-spring/three": "^9.0.0-rc.3",
		"d3": "^5.16.0",
		"md5": "^2.2.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-three-fiber": "^4.2.18",
		"remark": "^12.0.1",
		"remark-frontmatter": "^2.0.0",
		"remark-wiki-link": "^0.0.4",
		"three": "^0.119.1"
	}
}
